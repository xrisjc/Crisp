// Netwon's method to calculate the square roots.

var abs := function (x) begin
    if x < 0 then -x else x
end

var sqrt := function (x) begin
    var guess := x / 2.0
    while abs(guess * guess - x) >= 0.00001 do
        guess := 0.5 * (guess + x / guess)
    guess
end

var x := 1
while x <= 100 do begin
    write(x, ' -- ', sqrt(x), '\n')
    x := x + 1
end
